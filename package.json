{
    "name": "@iota/workspaces",
    "description": "IOTA JavaScript Library",
    "version": "2.0.0-rc.1",
    "keywords": [
        "iota"
    ],
    "authors": [
        "Martyn Janes <martyn.janes@iota.org>"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/iotaledger/iota.js.git"
    },
    "license": "Apache-2.0",
    "workspaces": [
        "packages/util",
        "packages/crypto",
        "packages/iota",
        "packages/mqtt",
        "packages/pow-neon",
        "packages/pow-node",
        "packages/pow-wasm/wasm",
        "packages/pow-wasm",
        "packages/pow-browser",
        "packages/iota/examples/address",
        "packages/iota/examples/data",
        "packages/iota/examples/peers",
        "packages/iota/examples/pow",
        "packages/iota/examples/simple",
        "packages/iota/examples/transaction",
        "packages/iota/examples/mint-native-tokens",
        "packages/iota/examples/mint-nft",
        "packages/iota/examples/native-token-tour",
        "packages/iota/examples/nft-collection",
        "packages/mqtt/examples/mqtt-client"
    ],
    "wikiWorkspaces": [
        "packages/util",
        "packages/crypto",
        "packages/iota",
        "packages/mqtt",
        "packages/pow-neon",
        "packages/pow-node",
        "packages/pow-wasm/wasm",
        "packages/pow-wasm",
        "packages/pow-browser"
    ],
    "dependencies": {
        "prettier": "^2.8.2"
    },
    "scripts": {
        "dist-neon": "npm run postinstall --prefix packages/pow-neon",
        "dist-all": "npm run dist-neon && npm run dist --workspaces",
        "build-wiki-api-ref": "npm run docs-wiki-build --workspace packages/util  --workspace packages/crypto  --workspace packages/iota   --workspace packages/mqtt  --workspace packages/pow-neon --workspace packages/pow-node --workspace packages/pow-wasm --workspace packages/pow-browser",
        "workspace-format": "prettier --config .prettierrc --write ."
    }
}
